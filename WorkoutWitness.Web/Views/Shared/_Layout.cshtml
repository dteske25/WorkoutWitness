<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WorkoutWitness_Web</title>
    <base href="~/" />
    <environment names="Development">
        <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/dist/site.css" asp-append-version="true" />
    </environment>
    <environment names="Production">
        <link rel="stylesheet" href="https://s3-us-west-2.amazonaws.com/workout-witness/vendor.css" asp-append-version="true" />
        <link rel="stylesheet" href="https://s3-us-west-2.amazonaws.com/workout-witness/site.css" asp-append-version="true" />
    </environment>
</head>
<body>
    @RenderBody()
    <environment names="Development">
        <script src="~/dist/vendor.js" asp-append-version="true"></script>
    </environment>
    <environment names="Production">
        <script src="https://s3-us-west-2.amazonaws.com/workout-witness/vendor.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("scripts", required: false)
</body>
</html>
